# Kiwoom API Configuration
# Copy this file to .env and fill in your actual credentials
# NEVER commit .env to git!

# 키움증권 계좌 정보
KIWOOM_ACCOUNT_NUMBER=your_account_number_here
KIWOOM_ACCOUNT_PASSWORD=your_account_password_here

# 키움 Open API 로그인 정보 (자동 로그인 사용시)
KIWOOM_USER_ID=your_kiwoom_id
KIWOOM_USER_PASSWORD=your_login_password
KIWOOM_CERT_PASSWORD=your_cert_password

# API 설정
KIWOOM_AUTO_LOGIN=false  # true로 설정시 자동 로그인 시도
KIWOOM_USE_MOCK=false    # 개발/테스트시 true로 설정하면 Mock 데이터 사용

# Google OAuth (기존)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Flask Configuration
FLASK_SECRET_KEY=your_secret_key_here
FLASK_ENV=development

# Stripe Payment (기존)
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key

# Database
DATABASE_PATH=web/database.db
